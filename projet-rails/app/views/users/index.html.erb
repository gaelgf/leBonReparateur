
<h1>Listing Users</h1>
<table>
  <thead>
    <tr>
      <th>email</th>
      <th>Nom</th>
      <th>Prenom</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.nom %></td>
        <td><%= user.prenom %></td>
      </tr>
    <% end %>
  </tbody>
</table>
